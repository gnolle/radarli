<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/material_pink_500</item>
        <item name="colorPrimaryDark">@color/material_pink_700</item>
        <item name="colorAccent">@color/material_accent_pink_200</item>
        <item name="android:textColorPrimary">@color/material_white</item>
    </style>

    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

</resources>
